{
  "name": "resap",
  "version": "0.0.1",
  "description": "React hooks for easy GSAP animations, cause why not?",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "build": "npm run lint && parcel build ./src/index.ts -d ./lib/",
    "dev": "parcel watch ./src/index.ts -d ./lib/",
    "prepublishOnly": "npm run build",
    "test": "nyc mocha -r ts-node/register ./test/**/*.spec.ts "
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phl3bas/Resap.git"
  },
  "keywords": [
    "npm",
    "node",
    "react",
    "javascript",
    "gsap",
    "animation",
    "animations",
    "hooks",
    "library"
  ],
  "author": "Sepp Wikramaratna-Clarke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Phl3bas/Resap/issues"
  },
  "homepage": "https://github.com/Phl3bas/Resap#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
